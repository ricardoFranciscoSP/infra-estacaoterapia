# URL permitida para requisições CORS
CORS_ENV=http://localhost:3000

# URL de conexão com o banco de dados PostgreSQL
DATABASE_URL=postgresql://usuario:senha@host:porta/nome_do_banco

# Chave secreta para geração de tokens JWT
JWT_SECRET=sua_chave_jwt_aqui

# Ambiente da aplicação (development, production, etc.)
NODE_ENV=development

# Porta em que a aplicação será executada
PORT=5000

# --- Configurações do Jitsi ---
JITSI_APP_ID=sua_app_id_do_jitsi
JITSI_SECRET=sua_chave_secreta_do_jitsi
JITSI_URL=https://seu_jitsi_url

# --- Configurações de e-mail ---
MAIL=seu_email@dominio.com
MAIL_HOST=smtp.seudominio.com
MAIL_PASSWORD=sua_senha_smtp
MAIL_PORT=2525
MAIL_USERNAME=seu_usuario_smtp

# --- Configurações da Supabase ---
SUPABASE_ANON_KEY=sua_chave_anon
SUPABASE_SERVICE_ROLE_KEY=sua_chave_role
SUPABASE_STORAGE_ACCESS_KEY_ID=seu_storage_access_key
SUPABASE_STORAGE_ENDPOINT=https://sua_url_storage.supabase.co
SUPABASE_STORAGE_REGION=us-west-1
SUPABASE_STORAGE_SECRET_KEY_ID=sua_storage_secret_key
SUPABASE_URL=https://sua_url_instancia.supabase.co

# Buckets do Supabase Storage
# Bucket privado para documentos (requer autenticação)
SUPABASE_BUCKET=estacao_upload_documents_pre
# Bucket público para imagens/avatares (URLs públicas)
SUPABASE_BUCKET_PUBLIC=estacao_public_uploads
# Bucket público para documentos de políticas e termos (URLs públicas)
SUPABASE_BUCKET_DOCUMENTS_PUBLIC=estacao_upload_documents_pub
